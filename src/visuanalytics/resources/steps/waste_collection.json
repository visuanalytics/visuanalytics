{
  "id": 1,
  "name": "waste_collection",
  "info": "Der Abfuhrkalender für die Gemeinde Biebertal.",
  "api": {
    "type": "request_multiple",
    "steps_value": [
      "Biebertal-Bieber",
      "Biebertal-Fellingshausen",
      "Biebertal-Frankenbach",
      "Biebertal-Königsberg",
      "Biebertal-Krumbach",
      "Biebertal-Rodheim",
      "Biebertal-Vetzberg"
    ],
    "url_pattern": "{_conf|url}/{_loop}.json",
    "use_loop_as_key": true
  },
  "transform": [
    {
      "type": "add_data",
      "new_keys": ["today"],
      "data": "heute"
    },
    {
      "type": "add_data",
      "new_keys": ["tomorrow"],
      "data": "morgen"
    },
    {
      "type": "add_data",
      "new_keys": ["and"],
      "data": " und "
    },
    {
      "type": "add_data",
      "new_keys": ["collectionToday"],
      "data": {
        "isEmpty": true
      }
    },
    {
      "type": "add_data",
      "new_keys": ["collectionTomorrow"],
      "data": {
        "isEmpty": true
      }
    },
    {
      "type": "transform_dict",
      "dict_key": "_req",
      "transform": [
        {
          "type": "add_data",
          "new_keys": ["city"],
          "data": "$_idx"
        },
        {
          "type": "replace",
          "keys": ["city"],
          "old_value": "Biebertal-",
          "new_value": ""
        },
        {
          "type": "transform_dict",
          "dict_key": "_loop",
          "filter_keys": ".*TAG$",
          "transform": [
            {
              "type": "compare",
              "value_left": "_loop|0",
              "value_right": "today",
              "on_equal": [
                {
                  "type": "append",
                  "keys": ["city"],
                  "new_keys": ["collectionToday|{_idx}"],
                  "append_type": "string",
                  "delimiter": ", "
                },
                {
                  "type": "add_data",
                  "new_keys": ["collectionToday|isEmpty"],
                  "data": false
                }
              ]
            },
            {
              "type": "compare",
              "value_left": "_loop|0",
              "value_right": "tomorrow",
              "on_equal": [
                {
                  "type": "append",
                  "keys": ["city"],
                  "new_keys": ["collectionTomorrow|{_idx}"],
                  "append_type": "string",
                  "delimiter": ", "
                },
                {
                  "type": "add_data",
                  "new_keys": ["collectionTomorrow|isEmpty"],
                  "data": false
                }
              ]
            }
          ]
        },
        {
          "type": "transform_dict",
          "dict_key": "_loop",
          "skip_keys": ".*TAG$",
          "transform": [
            {
              "type": "date_format",
              "keys": ["_loop|0"],
              "given_format": "%d.%m.%Y",
              "format": "%d.%m",
              "zeropaded_off": false
            }
          ]
        }
      ]
    },
    {
      "type": "add_data",
      "new_keys": ["collectionToday|text"],
      "data": "Heute werden die "
    },

    {
      "type": "add_data",
      "new_keys": ["todayAddAnd"],
      "data": false
    },
    {
      "type": "transform_dict",
      "dict_key": "collectionToday",
      "skip_keys": "isEmpty|text|Mobile Schadstoffsammlung im Ort TAG",
      "transform": [
        {
          "type": "regex",
          "keys": ["_idx"],
          "new_keys": ["tonne"],
          "regex": "(Tonne)? TAG",
          "replace_by": ""
        },
        {
          "type": "option",
          "check": "todayAddAnd",
          "on_true": [
            {
              "type": "append",
              "keys": ["and"],
              "new_keys": ["collectionToday|text"],
              "append_type": "string",
              "delimiter": ""
            }
          ]
        },
        {
          "type": "add_data",
          "new_keys": ["preText"],
          "data": "{tonne} Tonnen in {_loop} Abgeholt"
        },
        {
          "type": "append",
          "keys": ["preText"],
          "new_keys": ["collectionToday|text"],
          "append_type": "string",
          "delimiter": " "
        },
        {
          "type": "add_data",
          "new_keys": ["todayAddAnd"],
          "data": true
        }
      ]
    },
    {
      "type": "add_data",
      "new_keys": ["collectionTomorrow|text"],
      "data": "Morgen werden die "
    },
    {
      "type": "add_data",
      "new_keys": ["tomorrowAddAnd"],
      "data": false
    },
    {
      "type": "transform_dict",
      "dict_key": "collectionTomorrow",
      "skip_keys": "isEmpty|text|Mobile Schadstoffsammlung im Ort TAG",
      "transform": [
        {
          "type": "regex",
          "keys": ["_idx"],
          "new_keys": ["tonne"],
          "regex": "(Tonne)? TAG",
          "replace_by": ""
        },
        {
          "type": "option",
          "check": "tomorrowAddAnd",
          "on_true": [
            {
              "type": "append",
              "keys": ["and"],
              "new_keys": ["collectionTomorrow|text"],
              "append_type": "string",
              "delimiter": ""
            }
          ]
        },
        {
          "type": "add_data",
          "new_keys": ["preText"],
          "data": "{tonne} Tonnen in {_loop} Abgeholt"
        },
        {
          "type": "append",
          "keys": ["preText"],
          "new_keys": ["collectionTomorrow|text"],
          "append_type": "string",
          "delimiter": " "
        },
        {
          "type": "add_data",
          "new_keys": ["tomorrowAddAnd"],
          "data": true
        }
      ]
    }
  ],
