{
  "id": 0,
  "name": "weather_single",
  "api": {
    "type": "request",
    "api_key_name": "weatherbit",
    "url_pattern": "https://api.weatherbit.io/v2.0/forecast/daily",
    "params": {
      "postal_code": "{_conf|p_code}",
      "key": "{_api_key}",
      "country": "DE"
    }
  },
  "transform": [
    {
      "type": "select",
      "relevant_keys": [
        "_req|data"
      ]
    },
    {
      "type": "select_range",
      "array_key": "_req|data",
      "range_start": 0,
      "range_end": 5
    },
    {
      "type": "transform_array",
      "array_key": "_req|data",
      "transform": [
        {
          "type": "select",
          "array_key": "_loop|data",
          "relevant_keys": [
            "valid_date",
            "max_temp",
            "min_temp",
            "app_min_temp",
            "app_max_temp",
            "wind_cdir_full",
            "wind_spd",
            "weather|icon",
            "weather|code",
            "sunrise_ts",
            "sunset_ts",
            "rh",
            "pop"
          ]
        },
        {
          "type": "calculate",
          "action": "round",
          "keys": [
            "_loop|rh",
            "_loop|pop",
            "_loop|max_temp",
            "_loop|min_temp",
            "_loop|app_max_temp",
            "_loop|app_min_temp"
          ]
        },
        {
          "type": "replace",
          "keys": [
            "_loop|max_temp",
            "_loop|min_temp",
            "_loop|app_max_temp",
            "_loop|app_min_temp"
          ],
          "new_keys": [
            "_loop|str_max_temp",
            "_loop|str_min_temp",
            "_loop|str_app_max_temp",
            "_loop|str_app_min_temp"
          ],
          "old_value": ".",
          "new_value": ",",
          "count": 1
        },
        {
          "type": "calculate",
          "keys": [
            "_loop|wind_spd"
          ],
          "action": "multiply",
          "value_right": 3.6,
          "decimal": 2
        },
        {
          "type": "replace",
          "keys": [
            "_loop|wind_spd"
          ],
          "old_value": ".",
          "new_value": ",",
          "count": 1
        },
        {
          "type": "add_symbol",
          "keys": [
            "_loop|wind_spd"
          ],
          "new_keys": [
            "_loop|str_wind_spd"
          ],
          "pattern": "{_key} Kilometern pro Stunde"
        },
        {
          "type": "add_symbol",
          "keys": [
            "_loop|rh",
            "_loop|pop"
          ],
          "new_keys": [
            "_loop|str_rh",
            "_loop|str_pop"
          ],
          "pattern": "{_key} Prozent"
        },
        {
          "type": "date_weekday",
          "keys": [
            "_loop|valid_date"
          ],
          "new_keys": [
            "_loop|weekday"
          ],
          "given_format": "%Y-%m-%d"
        },
        {
          "type": "timestamp",
          "keys": [
            "_loop|sunset_ts",
            "_loop|sunrise_ts"
          ],
          "new_keys": [
            "_loop|str_sunset_ts",
            "_loop|str_sunrise_ts"
